{
  "name": "jet-template",
  "displayName": "jet-template",
  "description": "An open source VS Code extension for the Golang Jet Templating tool to provide basic syntax hylighting",
  "version": "0.0.2",
  "publisher": "telisoft",
  "icon": "icons/jet-icon.png",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jet",
      "aliases": ["Jet Template", "jet template", "jet-template"],
      "extensions": [".jet"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jet",
      "scopeName": "text.html.jet",
      "path": "./syntaxes/jet.tmLanguage.json",
      "injectTo": ["text.html.basic"]
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telisoft-uk/jet-vs-code"
  }
}